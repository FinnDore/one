{
    "build": {
        "devPath": "http://localhost:3000",
        "distDir": "../client/out",
        "withGlobalTauri": false
    },
    "package": {
        "productName": "one",
        "version": "0.0.0"
    },
    "tauri": {
        "macOSPrivateApi": true,
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            },
            "window": {
                "setFullscreen": true,
                "minimize": true,
                "close": true,
                "startDragging": true
            }
        },
        "bundle": {
            "active": true,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "k.finndore.dev",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "test2",
                "width": 800,
                "height": 600,
                "decorations": false,
                "transparent": true
            }
        ]
    }
}
